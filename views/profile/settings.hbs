 <div class="container-fluid">
      {{#each data}}
                <div class="row">
                        <div class="col-md-10 content">
                            <div class="row">
                                <div class="col-md-6 profile-content">
                                    <p class="display-4">Your Profile</p>
                                    <div class="form-group row">
                                        <label class="col-sm-2 col-form-label">Name</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" readonly value="{{this.fullname}}">
                                        </div>
                                    </div>
                                    <hr>
                                    <div class="form-group row">
                                        <label class="col-sm-2 col-form-label">Email</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" readonly value="{{this.mail}}">
                                        </div>
                                    </div>
                                    <hr>
                                    <div class="form-group row">
                                        <label class="col-sm-2 col-form-label">Phone</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" readonly value="{{this.phone}}">
                                        </div>
                                    </div>
                                    <hr>
                                    <div class="form-group row">
                                        <label class="col-sm-2 col-form-label">Address</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" readonly value="{{this.adress}}">
                                        </div>
                                    </div>
                                    <hr>
                                  
                                    
                                    <hr>
       {{/each}}

                                    <a href="/settings/edit">Edit Profile</a> |
                                    <a href="/settings/password">Change Password</a>

                                </div>
                               
                            </div>
                        </div>
                </div>
            </div>
